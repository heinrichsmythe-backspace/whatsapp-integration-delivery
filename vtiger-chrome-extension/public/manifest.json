{
  "manifest_version": 3,
  "name": "My Vue3 Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with Vue 3",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "vite.svg",
      "48": "vite.svg",
      "128": "vite.svg"
    }
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "assets/background.js"
  }
}
